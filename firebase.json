{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.git/**",
      "**/.github/**",
      "**/.gitignore",
      "**/.firebaserc",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
